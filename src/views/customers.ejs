                            <th>Veces_Escuchada</th>
                                      <th>Cant_User</th>
              </tr>
            </thead>
            <tbody>
              <% if (data) { %>
                <% for (var i = 0; i< data.length; i++) { %>
                <tr>
                   <td> <%= data[i].id_canc %></td>
                      <td> <%= data[i].nombre_canc %></td>
                      <td> <%= data[i].nombre_alb %></td>
                      <td> <%= data[i].nombreartistico_art %></td>
                      <td> <%= data[i].nombre_user %></td>
                      <td> <%= data[i].VecesEscuchada %></td>
                        <td> <%= data[i].CantUser %></td>
                </tr>
              <% } %>

              <% } %>
            </tbody>
          </table>
        </div>
          <div class="col-md-5">
          </div>
          <button type="submit" class="btn btn-primary">
          Vista1
        </button>

    </div>

</div>


