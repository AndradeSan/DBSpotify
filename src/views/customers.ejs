

<%- include('partials/_header'); %>


<div class="container">
    <div class="row mt-5">
        <div class="col-md-7">
          <table class="table table-bordered table-hover">
            <thead>
              <tr>
                <th>id_Cancion</th>
                    <th>Nombre_Cancion</th>
                      <th>Nombre_Albun</th>
                      <th>Nombre_Artista</th>
